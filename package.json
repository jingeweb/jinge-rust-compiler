{
  "name": "jinge-compiler",
  "version": "4.0.0",
  "description": "template compiler for jinge mvvm framework",
  "author": "Yuhang Ge<abeyuhang@gmail.com>",
  "license": "ISC",
  "type": "module",
  "main": "dist/index.js",
  "types": "ts/index.ts",
  "keywords": [
    "jinge",
    "compiler",
    "swc"
  ],
  "scripts": {
    "build-ts": "tsup ts/index.ts --format esm",
    "build-cargo-debug": "cargo build && cp target/debug/libjinge_compiler.so index.node 2>/dev/null || : && cp target/debug/libjinge_compiler.dylib index.node 2>/dev/null || :",
    "watch-cargo": "cargo watch -i lib -i ts -- pnpm build-cargo-debug",
    "dev": "concurrently \"pnpm build-ts --watch\" \"pnpm watch-cargo\"",
    "build": "tsc && pnpm build-ts --silent && echo 'Build ts done. Use github actions to build and release rust core!!'"
  },
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "ts",
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.2.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "npm:eslint-plugin-i@^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "jinge-compiler-core-macos-aarch64": "^1.0.0",
    "jinge-compiler-core-linux-aarch64": "^1.0.0",
    "jinge-compiler-core-macos-x86_64": "^1.0.0",
    "jinge-compiler-core-linux-x86_64": "^1.0.0",
    "jinge-compiler-core-windows-x86_64": "^1.0.0"
  }
}
